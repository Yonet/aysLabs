    "@angular/language-service": "^7.0.0",
    "@ngrx/store-devtools": "6.1.0",
    "@nrwl/builders": "6.4.0",
    "@nrwl/nx": "6.4.0",
    "@nrwl/schematics": "6.4.0",
    "@types/express": "4.16.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.0.0",
    "@types/node": "~8.9.4",
