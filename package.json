    "@angular/forms": "^7.0.0",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@ngrx/effects": "6.1.0",
    "@ngrx/entity": "6.1.0",
    "@ngrx/router-store": "6.1.0",
    "@ngrx/store": "6.1.0",
    "@nrwl/nx": "6.4.0",
    "core-js": "^2.5.4",
    "express": "4.16.3",
    "rxjs": "^6.3.3",
    "three": "^0.97.0",
    "three.ar.js": "^0.1.8",
    "webxr-polyfill": "^1.0.11",
